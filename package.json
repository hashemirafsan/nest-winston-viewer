{
  "name": "nest-winston-viewer",
  "version": "0.1.1",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": ["dist"],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json && cp src/viewer.static.html dist/",
    "prepare": "npm run build",
    "dev": "tsc -w -p tsconfig.json"
  },
  "peerDependencies": {
    "@nestjs/common": ">=10.0.0 <12",
    "@nestjs/core": ">=10.0.0 <12",
    "nest-winston": "^1.10.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^20.11.0",
    "express": "^5.1.0",
    "typescript": "^5.4.0"
  }
}
